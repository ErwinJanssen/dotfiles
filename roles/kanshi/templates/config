# If no outputs are connected, enable build-in display
profile {
	output eDP-1 enable
}

# Disable build-in display when one of these external monitors is connected
profile {
    # Desk at home
    output eDP-1 disable
    output "GIGA-BYTE TECHNOLOGY CO., LTD. G27Q 20362B001216" enable
}

profile {
    # Desk at office
    output eDP-1 disable
    output "Dell Inc. DELL S3422DW 4981TH3" mode 3440x1440
}

profile {
    # Flex desk at office
    output eDP-1 disable
    output "Dell Inc. DELL S3422DW 4CT0TH3" mode 3440x1440
}

profile {
    # Flex desk at office
    output eDP-1 disable
    output "Dell Inc. DELL S3422DW BMN0TH3" mode 3440x1440
}
